{
  "background": {
    "scripts": [ "js/bg.js" ]
  },
  "browser_action": {
    "default_icon": "img/share.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "img/share.png"
  },

  "manifest_version": 2,
  "name": "Auto Decker",
  "permissions": [ "<all_urls>", "tabs", "storage" ],
  "version": "1.0.2",
  "content_scripts": [
    {
      "matches": ["https://tweetdeck.twitter.com/*"],
      "js": ["js/jquery-1.12.4.min.js", "js/content.js"]
    }
  ]
}
